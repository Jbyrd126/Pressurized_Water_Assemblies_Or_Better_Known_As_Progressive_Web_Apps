{
  "name": "pressurized_water_assemblies",
  "version": "1.0.0",
  "description": "Text editor application converted to PWA",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"cd client && npm run dev\" \"cd server && npm run server\" ",
    "start": "npm run build && cd server && node server.js",
    "server": "cd server nodemon server.js --ignore client",
    "build": "cd client && npm run build",
    "install": "cd server && npm i && cd ../client && npm i",
    "client": "cd client && npm start"
  },
  "keywords": [],
  "author": "Rob Byrd",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jbyrd126/Pressurized_Water_Assemblies_Or_Better_Known_As_Progressive_Web_Apps"
  },
  "bugs": {
    "url": "https://github.com/Jbyrd126/Pressurized_Water_Assemblies_Or_Better_Known_As_Progressive_Web_Apps/issues"
  },
  "homepage": "https://github.com/Jbyrd126/Pressurized_Water_Assemblies_Or_Better_Known_As_Progressive_Web_Apps/blob/main/README.md",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4",
    "webpack-cli": "^4.8.0"
  }
}
